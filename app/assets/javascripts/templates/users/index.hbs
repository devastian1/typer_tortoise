<div class="container">
  <h1>User Index</h1>

  <div class="row">
    <div class="col-md-6">
      <table class="table table-striped table-bordered">
        <tr>
          <th>User ID</th>
          <th>Username</th>
        </tr>

        {{#each user in users}}
          <tr>
            <td>{{user.id}}</td>
            <td>{{#link-to 'user.index' user.id}}{{user.name}}{{/link-to}}</td>
          </tr>
        {{/each}}
      </table>
    </div>
  </div>
</div>