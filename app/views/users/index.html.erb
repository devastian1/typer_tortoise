<h1>User Index</h1>

<div class="row">
  <div class="col-md-6">
    <table class="table table-striped table-bordered">
      <tr>
        <th>User ID</th>
        <th>Username</th>
      </tr>

      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= link_to user.name, user %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
